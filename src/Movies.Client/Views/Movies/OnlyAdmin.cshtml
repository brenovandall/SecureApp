@model UserInfoViewModel

@{
    ViewData["Title"] = "OnlyAdmin";
}

<h1>OnlyAdmin</h1>

<table>
    <thead>
        <tr>
            <th>Claim type</th>
            <th>Claim value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.UserInfoDictionary)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Key);
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value);
                </td>
            </tr>
        }
    </tbody>
</table>